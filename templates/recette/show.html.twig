                            html = html + '<li>'
                            html = html + '<div class="avatar"><a href="javascript:void (0);">' +
                                '<img src="https://ui-avatars.com/api/?name='+comment.auteur.username+'&color=7F9CF5&background=EBF4FF" alt="avatar"/></a>' +
                                '</div>'
                            html = html + '<div class="comment"><h5><a href="javascript:void(0);">'+comment.auteur.username+'</a></h5>'
                            html = html + '<span class="time">('+comment.time_ago+')</span>' +
                                '<p>'+comment.content+'</p></div></li>'
                            console.log(comment);
                        });
                        $('#list-comments').html(html);
                    },
                    beforeSend: function () {
                        //console.log('SEND')
                        spinner.css("display", 'block');
                    }
                })
                .done(function () {
                    //console.log('DONE')
                    spinner.css("display", 'none');
                })
                .error(function () {
                    //console.log('ERROR')
                    spinner.css("display", 'none');
                })
                ;
            }

            function post_comment (comment) {
                let btn = $('#btn-post-comment');
                $.ajax({
                    url: '/comment/create',
                    type: 'post',
                    data: {recipe: comment.recipe, content: comment.content},
                    dataType: 'json',
                    success: function(comment){
                        let listComments = $('#list-comments');
                        let element = '';

                        element = element + '<li>'
                        element = element + '<div class="avatar"><a href="javascript:void (0);">' +
                            '<img src="https://ui-avatars.com/api/?name='+comment.auteur.username+'&color=7F9CF5&background=EBF4FF" alt="avatar"/></a>' +
                            '</div>'
                        element = element + '<div class="comment"><h5><a href="javascript:void(0);">'+comment.auteur.username+'</a></h5>'
                        element = element + '<span class="time">('+comment.time_ago+')</span>' +
                            '<p>'+comment.content+'</p></div></li>'


                        listComments.prepend(element)
                        let total_comments = document.getElementById("list-comments").getElementsByTagName("li").length
                        $('#total_comments').text('Comments ('+total_comments+')')
                    },
                    beforeSend: function () {
                        //console.log('SEND')
                        btn.attr("disabled", true);
                    }
                })
                .done(function () {
                    btn.attr("disabled", false);
                })
                .error(function () {
                    //console.log('ERROR')
                    btn.attr("disabled", false);
                })
                ;
            }

            $('#comment-form').on("submit", function (e) {
                e.preventDefault();
                let comment = {
                    'recipe': $('#recipe').val(),
                    'content': $('#content').val(),
                }
                post_comment(comment)
                //console.log(comment)
            });
        });
    </script>

{% endblock %}
